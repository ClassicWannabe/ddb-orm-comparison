    "@aws-sdk/lib-dynamodb": "^3.418.0",
    "@typedorm/common": "^1.15.4",
    "@typedorm/core": "^1.15.4",
    "dynamodb-onetable": "^2.7.1",
    "dynamodb-toolbox": "^0.8.5",
    "dynamoose": "^3.2.1",
    "electrodb": "^2.10.1",
    "reflect-metadata": "^0.1.13"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": "< 3.0.0",
    "node": ">= 18.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "devDependencies": {
    "@types/node": "^20.6.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
