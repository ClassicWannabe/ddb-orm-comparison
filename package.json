{
  "name": "ddb-orm-comparison",
  "version": "1.0.0",
  "description": "Comparison between popular DDB ORMs",
  "main": "index.ts",
  "scripts": {
    "db:local": "sudo docker-compose up",
    "init:sdk": "ts-node src/sdk/index.ts",
    "init:dynamodb-toolbox": "ts-node src/dynamodb-toolbox/index.ts",
    "init:dynamoose": "ts-node src/dynamoose/index.ts",
    "init:typedorm": "ts-node src/typedorm/index.ts",
    "init:electrodb": "ts-node src/electrodb/index.ts",
    "init:dynamodb-onetable": "ts-node src/dynamodb-onetable/index.ts"
  },
  "keywords": [
    "orm",
    "dynamodb",
    "nodejs",
    "typescript"
  ],
  "author": "Ruslan Yeleussinov",
  "license": "MIT",
  "dependencies": {
